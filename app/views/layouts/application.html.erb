<!DOCTYPE html>
<html>
<head>
 	<title>Achinamas</title>
  	<!-- CSS Global Compulsory-->
  	<%= stylesheet_link_tag 'style.css'%>
  	<%= stylesheet_link_tag 'theme-colors/default.css' %>
   	<!-- CSS Implementing Plugins --> 
   	<%= stylesheet_link_tag 'plugins/revolution-slider/settings.css', media: 'screen' %>
   	<!--[if lt IE 9]><%= stylesheet_link_tag 'plugins/revolution-slider/settings-ie8.css', media: 'screen' %><![endif]-->

  	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  	<%= csrf_meta_tags %>
	<meta name="viewport" content="width=device-width, initial-scale=1"/>
</head>

<body>
	<%= render partial: "elements/navbar" %>
	<%= bootstrap_flash %>


	<%= yield %>

	<!-- RENDER FOOTER HERE -->


	<!-- JS Global Compulsory --> 
	<%= javascript_include_tag 'plugins/jquery/jquery.min.js' %>
	<%= javascript_include_tag 'plugins/jquery/jquery-migrate.min.js' %>
	<%= javascript_include_tag 'plugins/bootstrap/js/bootstrap.min.js' %>
	<!-- JS Implementing Plugins-->
	<%= javascript_include_tag 'plugins/back-to-top.js' %>
	<%= javascript_include_tag 'plugins/revolution-slider/jquery.themepunch.tools.min.js' %>
	<%= javascript_include_tag 'plugins/revolution-slider/jquery.themepunch.revolution.min.js' %>
	<!-- JS Customization -->
	<%= javascript_include_tag 'custom.js' %>
	<!-- JS Page Level: MAY HAVE TO GO INTO SPECIFIC VIEWS NOT GLOBAL SETTINGS-->
	<%= javascript_include_tag 'app.js' %>
	<%= javascript_include_tag 'plugins/revolution-slider.js' %>
	<script type="text/javascript">
	    jQuery(document).ready(function() {
	        App.init();
	        //OwlCarousel.initOwlCarousel();        
	        RevolutionSlider.initRSfullWidth();
	    });
	</script>


	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
</body>
</html>
